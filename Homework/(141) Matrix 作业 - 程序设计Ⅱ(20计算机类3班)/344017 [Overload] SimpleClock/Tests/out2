cin >> clock1
>> clock1 ---- 18:36:54

clock2(clock1)
>> clock1 ---- 18:36:54
>> clock2 ---- 18:36:54

clock3 = clock1 ++
clock4 = ++ clock2
>> clock1 ---- 18:36:55
>> clock2 ---- 18:36:55
>> clock3 ---- 18:36:54
>> clock4 ---- 18:36:55

clock5(11, 45, 14)
clock6 = clock1 + clock5
>> clock1 ---- 18:36:55
>> clock5 ---- 11:45:14
>> clock6 ---- 6:22:9

clock7()
clock7 += clock5
clock7 += clock6
>> clock5 ---- 11:45:14
>> clock6 ---- 6:22:9
>> clock7 ---- 18:7:23
